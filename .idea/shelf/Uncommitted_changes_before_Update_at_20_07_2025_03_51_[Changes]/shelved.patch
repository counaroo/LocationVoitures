+package com.couro.sadio.locationvoitures.controller.adminControllers;
+
+import com.couro.sadio.locationvoitures.dao.impl.HibernateClientDaoImpl;
+import com.couro.sadio.locationvoitures.dao.impl.HibernateReservationDaoImpl;
+import com.couro.sadio.locationvoitures.dao.impl.HibernateVehiculeDaoImpl;
+import com.couro.sadio.locationvoitures.entities.Client;
+import com.couro.sadio.locationvoitures.entities.Reservation;
+import com.couro.sadio.locationvoitures.entities.Vehicule;
+import javafx.fxml.FXML;
+import javafx.scene.control.Alert;
+import javafx.scene.control.ComboBox;
+import javafx.scene.control.DatePicker;
+
+import java.time.LocalDate;
+import java.util.List;
+
+public class UIReservationController {
+
+    @FXML
+    private ComboBox<Client> clientComboBox;
+
+    @FXML
+    private ComboBox<Vehicule> vehiculeComboBox;
+
+    @FXML
+    private DatePicker dateDebutPicker;
+
+    @FXML
+    private DatePicker dateFinPicker;
+
+    private final HibernateObjectDaoImpl<Client> clientDao = new HibernateObjectDaoImpl<>(Client.class);
+    private final HibernateObjectDaoImpl<Vehicule> vehiculeDao = new HibernateObjectDaoImpl<>(Vehicule.class);
+    private final HibernateObjectDaoImpl<Reservation> reservationDao = new HibernateObjectDaoImpl<>(Reservation.class);
+
+
+
+    @FXML
+    public void initialize() {
+        List<Client> clients = clientDao.findAll();
+        clientComboBox.getItems().addAll(clients);
+
+        List<Vehicule> vehicules = vehiculeDao.findAllDispos();
+        vehiculeComboBox.getItems().addAll(vehicules);
+    }
+
+    @FXML
+    public void handleReserverAction() {
+        Client client = clientComboBox.getValue();
+        Vehicule vehicule = vehiculeComboBox.getValue();
+        LocalDate debut = dateDebutPicker.getValue();
+        LocalDate fin = dateFinPicker.getValue();
+
+        if (client == null || vehicule == null || debut == null || fin == null) {
+            showAlert("Veuillez remplir tous les champs.");
+            return;
+        }
+
+        Reservation reservation = new Reservation();
+        reservation.setClient(client);
+        reservation.setVehicule(vehicule);
+        reservation.setDateDebut(java.sql.Date.valueOf(debut));
+        reservation.setDateFin(java.sql.Date.valueOf(fin));
+        reservation.setStatut("EN_ATTENTE");
+
+        reservationDao.save(reservation);
+        showAlert("Réservation enregistrée avec succès.");
+    }
+
+    private void showAlert(String message) {
+        Alert alert = new Alert(Alert.AlertType.INFORMATION);
+        alert.setTitle("Information");
+        alert.setHeaderText(null);
+        alert.setContentText(message);
+        alert.showAndWait();
+    }
+}
